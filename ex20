class Rextester
{          
	    int id;  
	    String name;  
	      
	    Rextester(int id,String name){  
	    this.id = id;  
	    this.name = name;  
	    }  
	    void display(){System.out.println(id+" "+name);}  
	  
	    public static void main(String args[]){  
	    Rextester s1 = new Rextester(111,"preethi");  
	    Rextester s2 = new Rextester(321,"bavani");  
	    s1.display();  
	    s2.display();  
	    }  
	}  
